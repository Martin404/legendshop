<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--  LegendShop 版权所有, 并保留所有权利。 -->
<hibernate-mapping>
    <class name="com.legendshop.model.entity.Event" table="ls_event">
        <id name="eventId" type="java.lang.Long">
            <column name="event_id" />
            <generator class="native" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="user_id" not-null="true" length="32" >
                <comment>操作员ID</comment>
            </column>
        </property>
        <property name="userName" type="java.lang.String">
            <column name="user_name" length="50" not-null="true">
                <comment>操作员名称</comment>
            </column>
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" length="1">
                <comment>操作类型</comment>
            </column>
        </property>
        <property name="operation" type="java.lang.String">
            <column name="operation" length="50">
                <comment>操作描述</comment>
            </column>
        </property>
        <property name="relateData" type="java.lang.String">
            <column name="relate_data" length="65535">
                <comment>相关数据</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="create_time" length="19" not-null="true">
                <comment>建立时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
