<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--  LegendShop 版权所有 2009-2011,并保留所有权利。 -->
<hibernate-mapping>
    <class name="com.legendshop.model.entity.ShopDetail" table="ls_shop_detail">
        <id name="shopId" type="java.lang.Long">
            <column name="shop_id"/>
            <generator class="native" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="user_id" length="32">
                <comment>userId</comment>
            </column>
        </property>
        <property name="web" type="java.lang.String">
            <column name="web" length="50">
                <comment>web地址</comment>
            </column>
        </property>
        <property name="sitename" type="java.lang.String">
            <column name="sitename" length="50">
                <comment>站点名称</comment>
            </column>
        </property>
        <property name="maddr" type="java.lang.String">
            <column name="maddr" length="300">
                <comment>网店地址</comment>
            </column>
        </property>
        <property name="msn" type="java.lang.String">
            <column name="msn" length="50">
                <comment>银行汇款帐号</comment>
            </column>
        </property>
        <property name="mname" type="java.lang.String">
            <column name="mname" length="50">
                <comment>收款人姓名</comment>
            </column>
        </property>
        <property name="code" type="java.lang.String">
            <column name="code" length="100">
                <comment>邮政编码</comment>
            </column>
        </property>
        <property name="ymaddr" type="java.lang.String">
            <column name="ymaddr" length="300">
                <comment>邮局汇款地址</comment>
            </column>
        </property>
        <property name="ymname" type="java.lang.String">
            <column name="ymname" length="100">
                <comment>邮递接收人</comment>
            </column>
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="status">
                <comment>状态,是否上线1：在线，0下线，-1审核中,-2拒绝</comment>
            </column>
        </property>
        <property name="storeName" type="java.lang.String">
            <column name="store_name" length="100">
                <comment>店名</comment>
            </column>
        </property>
        <property name="visitTimes" type="java.lang.Integer">
            <column name="visit_times">
                <comment>访问人数</comment>
            </column>
        </property>
        <property name="productNum" type="java.lang.Integer">
            <column name="product_num">
                <comment>产品数量</comment>
            </column>
        </property>
        <property name="offProductNum" type="java.lang.Integer">
            <column name="off_product_num">
                <comment>下线产品数量</comment>
            </column>
        </property>
        <property name="commNum" type="java.lang.Integer">
            <column name="comm_num">
                <comment>用户评论数量</comment>
            </column>
        </property>
        <property name="modifyTime" type="java.util.Date">
            <column name="modify_time" length="19" not-null="true">
                <comment>修改时间</comment>
            </column>
        </property>
        <property name="addtime" type="java.util.Date">
            <column name="addtime" length="19" not-null="true">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="briefDesc" type="java.lang.String">
            <column name="brief_desc" length="300">
                <comment>店铺简述</comment>
            </column>
        </property>
        <property name="detailDesc" type="java.lang.String">
            <column name="detail_desc" length="4000">
                <comment>店铺详细描述</comment>
            </column>
        </property>
        <property name="shopPic" type="java.lang.String">
            <column name="shop_pic" length="100">
                <comment>店铺图片</comment>
            </column>
        </property>
        <property name="colorStyle" type="java.lang.String">
            <column name="color_style" length="15">
                <comment>风格类型</comment>
            </column>
        </property> 
        <property name="langStyle" type="java.lang.String">
            <column name="lang_style" length="15">
                <comment>语言类型</comment>
            </column>
        </property>  
        <property name="gradeId" type="java.lang.Integer">
            <column name="grade_id">
                <comment>商铺等级</comment>
            </column>
        </property> 
        <property name="type" type="java.lang.Integer">
            <column name="type">
                <comment>商铺等级</comment>
            </column>
        </property> 
        <property name="idCardPic" type="java.lang.String">
            <column name="id_card_pic" length="250">
                <comment>身份证验证图片</comment>
            </column>
        </property>  
        <property name="trafficPic" type="java.lang.String">
            <column name="traffic_pic" length="250">
                <comment>营业执照图片</comment>
            </column>
        </property> 
        <property name="idCardNum" type="java.lang.String">
            <column name="id_card_num" length="20">
                <comment>身份证号码</comment>
            </column>
        </property> 
        <property name="createCountryCode" type="java.lang.String">
            <column name="create_country_code" length="20">
                <comment>创建时的国家码</comment>
            </column>
        </property>
        <property name="createAreaCode" type="java.lang.String">
            <column name="create_area_code" length="20">
                <comment>创建时的地区</comment>
            </column>
        </property> 
        <property name="provinceid" type="java.lang.Integer">
            <column name="provinceid">
                <comment>省份</comment>
            </column>
        </property>
        <property name="cityid" type="java.lang.Integer">
            <column name="cityid">
                <comment>城市</comment>
            </column>
        </property> 
        <property name="areaid" type="java.lang.Integer">
            <column name="areaid">
                <comment>地区</comment>
            </column>
        </property> 
    </class>
</hibernate-mapping>
