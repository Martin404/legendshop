<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--  LegendShop 版权所有 2009-2011,并保留所有权利。 -->
<hibernate-mapping>
    <class name="com.legendshop.model.entity.UserDetail" table="ls_usr_detail">
        <id name="userId" type="java.lang.String">
            <column name="user_id" />
            <generator class="assigned" />
        </id>
        <property name="gradeId" type="java.lang.Integer">
            <column name="grade_id">
                <comment>等级ID</comment>
            </column>
        </property>
        <property name="userName" type="java.lang.String">
            <column name="user_name" length="50" not-null="true">
                <comment>用户名称</comment>
            </column>
        </property>
        <property name="nickName" type="java.lang.String">
            <column name="nick_name" length="50" not-null="true">
                <comment>昵称</comment>
            </column>
        </property>
        <property name="userMail" type="java.lang.String">
            <column name="user_mail" length="50">
                <comment>用户邮件</comment>
            </column>
        </property>
        <property name="userAdds" type="java.lang.String">
            <column name="user_adds">
                <comment>用户地址</comment>
            </column>
        </property>
        <property name="userTel" type="java.lang.String">
            <column name="user_tel" length="50">
                <comment>电话，座机</comment>
            </column>
        </property>
        <property name="userMobile" type="java.lang.String">
            <column name="user_mobile" length="50">
                <comment>手机</comment>
            </column>
        </property>
        <property name="userPostcode" type="java.lang.String">
            <column name="user_postcode" length="50">
                <comment>用户邮编</comment>
            </column>
        </property>
        <property name="msn" type="java.lang.String">
            <column name="msn" length="50">
                <comment>msn</comment>
            </column>
        </property>
        <property name="qq" type="java.lang.String">
            <column name="qq" length="50">
                <comment>qq号码</comment>
            </column>
        </property>
        <property name="fax" type="java.lang.String">
            <column name="fax" length="50">
                <comment>传真</comment>
            </column>
        </property>
        <property name="modifyTime" type="java.util.Date">
            <column name="modify_time" length="19" not-null="true">
                <comment>修改时间</comment>
            </column>
        </property>
        <property name="userRegtime" type="java.util.Date">
            <column name="user_regtime" length="19">
                <comment>注册时间</comment>
            </column>
        </property>
        <property name="userRegip" type="java.lang.String">
            <column name="user_regip" length="50">
                <comment>注册IP</comment>
            </column>
        </property>
        <property name="userLasttime" type="java.util.Date">
            <column name="user_lasttime" length="10">
                <comment>最后登录时间</comment>
            </column>
        </property>
        <property name="userLastip" type="java.lang.String">
            <column name="user_lastip" length="50">
                <comment>最后登录IP</comment>
            </column>
        </property>
        <property name="userMemo" type="java.lang.String">
            <column name="user_memo" length="500">
                <comment>备注</comment>
            </column>
        </property>
        <property name="sex" type="java.lang.String">
            <column name="sex" length="1">
                <comment>性别</comment>
            </column>
        </property>
        <property name="birthDate" type="java.lang.String">
            <column name="birth_date" length="8">
                <comment>生日</comment>
            </column>
        </property>
        <property name="registerCode" type="java.lang.String">
            <column name="register_code" length="200">
                <comment>注册时生成的注册码</comment>
            </column>
        </property>
        <property name="score" type="java.lang.Long">
            <column name="score">
                <comment>用户积分</comment>
            </column>
        </property>       
    </class>
</hibernate-mapping>
